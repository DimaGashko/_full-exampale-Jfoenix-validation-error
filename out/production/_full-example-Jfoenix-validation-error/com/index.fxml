<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTextField?>
<ScrollPane fitToHeight="true" fitToWidth="true" stylesheets="@index.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.Index">
    <HBox>
        <VBox styleClass="group">
            <Label styleClass="group__title" text="No Error" />

            <JFXTextField fx:id="noAlign__fxField1" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 1" />
            <JFXTextField fx:id="noAlign__fxField2" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 2" />
            <JFXTextField fx:id="noAlign__fxField3" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 3" />
            <JFXTextField fx:id="noAlign__fxField4" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 4" />
            <JFXTextField fx:id="noAlign__fxField5" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 5" />
            <JFXTextField fx:id="noAlign__fxField6" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 6" />
        </VBox>
        <VBox styleClass="group">
            <Label styleClass="group__title" text="With error" />

            <JFXTextField fx:id="error__fxField1" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 1" />
            <JFXTextField fx:id="error__fxField2" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 2" />
            <JFXTextField fx:id="error__fxField3" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 3" />
            <JFXTextField fx:id="error__fxField4" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 4" />
            <JFXTextField fx:id="error__fxField5" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 5" />
            <JFXTextField fx:id="error__fxField6" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 6" />
        </VBox>
        <VBox styleClass="group">
            <Label styleClass="group__title" text="Fixed" />

            <JFXTextField fx:id="fixed__fxField1" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 1" />
            <JFXTextField fx:id="fixed___fxField2" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 2" />
            <JFXTextField fx:id="fixed___fxField3" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 3" />
            <JFXTextField fx:id="fixed___fxField4" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 4" />
            <JFXTextField fx:id="fixed___fxField5" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 5" />
            <JFXTextField fx:id="fixed___fxField6" labelFloat="true" prefHeight="40.0" prefWidth="100.0" promptText="Text Filed 6" />
        </VBox>
    </HBox>
</ScrollPane>
